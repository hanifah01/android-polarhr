<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_polar"
    tools:context=".view.pelatih.HasilLatihanPelatih">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_hasil_latihan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/ToolbarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/lyt_pilihtanggal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black_polar"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_hasil_latihan">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:text="Pilih tanggal"
            android:textSize="@dimen/font_small"
            android:fontFamily="@font/montserrat_medium"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp">
            <ImageView
                android:id="@+id/img_calendar_latihan"
                android:layout_width="@dimen/xxklarge"
                android:layout_height="@dimen/xxklarge"
                android:background="@drawable/bg_red_line"
                android:elevation="@dimen/small"
                android:src="@drawable/ic_date_white_24dp"
                android:padding="@dimen/semi_small"
                android:layout_gravity="center"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg_transparent"
                android:layout_marginLeft="@dimen/semi_small"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:textSize="15sp"
                    android:text="Pilih tanggal : "
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center_vertical"
                    android:paddingLeft="15dp" />

                <TextView
                    android:id="@+id/edt_calendar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textSize="15sp"
                    android:text="-"
                    android:textColor="@color/white"
                    android:fontFamily="@font/montserrat_medium"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/tiny"/>
            </LinearLayout>

        </LinearLayout>
        <TextView
            android:id="@+id/tampilkan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tampilkan"
            android:padding="10dp"
            android:background="@drawable/bg_red_button"
            android:textColor="@color/white"
            android:fontFamily="@font/montserrat_medium"
            android:gravity="center"
            android:layout_marginRight="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="25dp" />
    </LinearLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:fillViewport="true"
        app:layout_constraintTop_toBottomOf="@+id/lyt_pilihtanggal">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|center"
            android:orientation="vertical"
            android:fillViewport="true">
